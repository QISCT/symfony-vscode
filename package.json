{
    "name": "symfony-vscode",
    "displayName": "symfony-vscode",
    "description": "Debug and autocomplete of Symfony container",
    "version": "0.0.1",
    "publisher": "TheNouillet",
    "engines": {
        "vscode": "^1.25.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onView:containerView"
    ],
    "main": "./out/extension",
    "contributes": {
        "views": {
            "explorer": [
                {
                    "id": "containerView",
                    "name": "Container view"
                }
            ]
        },
        "commands": [
            {
                "command": "symfony-vscode.refreshView",
                "title": "Refresh Symfony container view"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test"
    },
    "devDependencies": {
        "typescript": "^2.6.1",
        "vscode": "^1.1.6",
        "@types/node": "^7.0.43",
        "@types/mocha": "^2.2.42"
    }
}